<h1>Articles</h1>

<ul>
  <% @articles.each do |article| %>
    <li>
      <%= article.title %>
      <%= link_to "Show Article", article, :class => "btn" %>
      <%= link_to "Edit", edit_article_path(article) %>
      <%= link_to "Destroy", article_path(article), method: :delete, data: { confirm: "Are you sure?" } %></li>
  <% end %>
</ul>
<div style="margin-left:1042px; margin-top:-94px;">
  <%= link_to "New Article", new_article_path %>
</div>